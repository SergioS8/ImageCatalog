{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "sourceMap": true,
    "removeComments": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "baseUrl": "./Frontend/js",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "outDir": "generated"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}